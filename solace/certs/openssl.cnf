# OpenSSL CA configuration file

[ req ]
default_bits       = 4096
default_md         = sha256
default_keyfile    = private.key
encrypt_key        = yes
prompt             = no
distinguished_name = distinguished_name

[ distinguished_name ]
countryName            = US
stateOrProvinceName    = NY
localityName           = New York
organizationName       = Solace Corporation
organizationalUnitName = Solutions Engineering
commonName             = Solace Corporation